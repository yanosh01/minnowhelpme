{
  "scripts": {
    "dev": "NODE_ENV=development nodemon -w src --exec \"babel-node src --presets env\"",
    "build": "babel src -s -D -d dist --presets env",
    "start": "node dist/index.js",
    "lint": "eslint src",
    "prettier": "prettier --write --single-quote --print-width=120 --tab-width=2 \"src/**/*.{js,jsx}\""
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.18.1",
    "nodemon": "^1.15.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "convert-seconds": "^1.0.1",
    "discord.js": "^11.3.0",
    "dotenv": "^5.0.0",
    "mongoose": "^4.13.9",
    "steem": "^0.7.1",
    "winston": "^2.4.0"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-var": 1
    }
  },
  "engines": {
    "node": "8.9.2"
  },
  "name": "minnowhelpme",
  "version": "1.0.0",
  "description": "Upvote bot on Steem Blockchain for Discord",
  "main": "index.js",
  "repository": "git@github.com:yanosh01/minnowhelpme.git",
  "author": "Yanos01",
  "license": "GNU"
}
